class Solution(object):
    def reverseWords(self, s):
        l = s.split()
        result = ''
        x = len(l)
        x -= 1
        while x >= 0:
            if x == 0:
                result += l[x]
            else:
                result += l[x]
                result += " "
            x-= 1
        return result
        
